// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:pattaya.db"
}

model Bot {
  id    Int     @default(autoincrement()) @id
  socketId String?
  wanIp String?
  lanIp String?
  os String?
  username String?
  hostname String?
  processName String?
  processId Decimal?
  architecture String?
  integrity String?
  country String?
  lastSeen DateTime? @updatedAt 
  hwid String? @unique
}